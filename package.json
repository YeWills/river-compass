{
  "name": "river-compass",
  "version": "0.0.4",
  "description": "A collection of configuration files containing prettier, eslint, stylelint",
  "keywords": [
    "eslint",
    "prettier",
    "antd",
    "ant",
    "design",
    "pro"
  ],
  "bin": {
    "compass-lint": "./utils/cli.js"
  },
  "homepage": "",
  "bugs": {
    "url": ""
  },
  "license": "ISC",
  "main": "dist/index.js",
  "files": [
    "dist",
    "utils"
  ],
  "scripts": {
    "build-tsc": "tsc --build tsconfig.json && node ./tool.js",
    "build": "babel src --out-dir dist  --extensions \".ts,.js\"  && node ./tool.js",
    "deps": "yarn upgrade-interactive --latest",
    "lib": "cross-env TIMING=1 eslint lib",
    "lint": "eslint --fix src utils",
    "prettier": "prettier -c --write \"**/*\"",
    "testTS": "npm run build && cd tsExample && npm run lint",
    "testJS": "npm run build && cd jsExample && npm run lint",
    "test": "npm run testTS && npm run testJS"
  },
  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/preset-typescript": "^7.12.7",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "chalk": "^4.1.1",
    "cowsay2": "^2.0.4",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-unicorn": "^20.0.0",
    "fast-glob": "^3.2.4",
    "inquirer": "^4.0.0",
    "stylelint": "^14.1.0",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-no-unsupported-browser-features": "^4.1.4",
    "stylelint-order": "^4.0.0",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "np": "^6.2.5",
    "prettier": "^2.2.1",
    "eslint": "^8.4.1",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/plugin-proposal-class-properties": "^7.14.5"
  },
  "peerDependencies": {
    "eslint": "^8.4.1",
    "prettier": "^2.2.1",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/plugin-proposal-class-properties": "^7.14.5"
  }
}
